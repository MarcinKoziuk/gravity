	background-top-image:    spacer2.tga         240px  0px 259px 10px;
	background-center-image: spacer2.tga stretch 240px 10px 259px 92px;
	background-bottom-image: spacer2.tga         240px 92px 259px 102px;*/
	background: #333;
}

scrollbarvertical sliderbar
{
    /*margin-left: 4px;*/
    width: 20px;
    min-height: 46px;

    background-decorator: tiled-vertical;
	background-top-image:    spacer2.tga         240px  0px 260px 11px;
	background-center-image: spacer2.tga stretch 240px 10px 260px 93px;
	background-bottom-image: spacer2.tga         240px 92px 260px 103px;
}

scrollbarvertical sliderbar:hover
{
    background-top-image-s: 200px 220px;
    background-center-image-s: 200px 220px;
    background-bottom-image-s: 200px 220px;
}

scrollbarvertical sliderbar:active
{
    background-top-image-s: 220px 240px;
    background-center-image-s: 220px 240px;
    background-bottom-image-s: 220px 240px;
}

scrollbarvertical sliderarrowdec
{
    width: 20px;
    height: 20px;
    
    icon-decorator: image;
    icon-image: spacer2.tga 240px 103px 260px 123px;
}

scrollbarvertical sliderarrowdec:hover,
scrollbarvertical sliderarrowinc:hover
{
    icon-image-s: 200px 220px;
}

scrollbarvertical sliderarrowdec:active,
scrollbarvertical sliderarrowinc:active
{
    icon-image-s: 220px 240px;
}

scrollbarvertical sliderarrowinc
{
    width: 20px;
    height: 20px;
    
    icon-decorator: image;
    icon-image: spacer2.tga 240px 123px 260px 143px;
}

scrollbarhorizontal
{
    width: 0px;
    height: 0px;
}