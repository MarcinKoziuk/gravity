    background-top-image:           spacer.tga stretch  205px 0px  390px 10px;
    background-bottom-left-image:   spacer.tga          195px 90px 205px 101px;
    background-bottom-right-image:  spacer.tga          390px 90px 401px 101px;
    background-bottom-image:        spacer.tga stretch  205px 90px 390px 101px;
